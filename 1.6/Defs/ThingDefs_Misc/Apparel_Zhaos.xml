<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef Name="ApparelBase_Zhaos" Abstract="True">
    <thingClass>Apparel</thingClass>
    <category>Item</category>
    <selectable>True</selectable>
    <pathCost>10</pathCost>
    <useHitPoints>True</useHitPoints>
    <techLevel>Ultra</techLevel>
    <graphicData>
      <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
    </graphicData>
    <drawGUIOverlay>true</drawGUIOverlay>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <Flammability>0</Flammability>
      <DeteriorationRate>0</DeteriorationRate>
      <Beauty>10</Beauty>
      <SellPriceFactor>0.70</SellPriceFactor>
    </statBases>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>True</alwaysHaulable>
    <tickerType>Never</tickerType>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
    <tradeTags>
      <li>Exotic</li>
    </tradeTags>
    <tradeability>Sellable</tradeability>
  </ThingDef>
  <ThingDef Name="ApparelMakeableBase_Zhaos" ParentName="ApparelBase_Zhaos" Abstract="True">
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
        <li>ElectricSmithy</li>
        <li>FueledSmithy</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
    </recipeMaker>
  </ThingDef>
  <ThingDef ParentName="ApparelMakeableBase_Zhaos">
    <defName>Z_Apparel_Zhaos</defName>
    <label>Z_Apparel_Zhaos.</label>
    <description>高强度抗性的内衬</description>
    <graphicData>
      <texPath>Things/Clothes/ZhaosC1/ZhaosC1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costList>
      <Z_StanStoneA>1</Z_StanStoneA>
    </costList>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <MarketValue>1</MarketValue>
      <MaxHitPoints>600</MaxHitPoints>
      <WorkToMake>20</WorkToMake>
      <Mass>1.0</Mass>
      <ArmorRating_Blunt>1.</ArmorRating_Blunt>
      <ArmorRating_Sharp>1.</ArmorRating_Sharp>
      <ArmorRating_Heat>1.</ArmorRating_Heat>
      <Insulation_Cold>160</Insulation_Cold>
      <Insulation_Heat>160</Insulation_Heat>
      <Beauty>100</Beauty>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>0.1</MoveSpeed>
      <WorkSpeedGlobal>0.1</WorkSpeedGlobal>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Waist</li>
        <li>Arms</li>
        <li>Legs</li>
        <li>Neck</li>
        <li>FullHead</li>
        <li>Hands</li>
        <li>Feet</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Clothes/ZhaosC1/ZhaosC1</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>100</weight>
          <only>RGBA(0.12,0.12,0.12,1)</only>
        </li>
      </options>
    </colorGenerator>
    <generateCommonality>0</generateCommonality>
    <generateAllowChance>0.00</generateAllowChance>
    <verbs>
      <li>
        <verbClass>Verb_CastTargetEffectLances</verbClass>
        <label>shard shock lance</label>
        <hasStandardCommand>true</hasStandardCommand>
        <targetable>true</targetable>
        <onlyManualCast>True</onlyManualCast>
        <range>16.9</range>
        <warmupTime>0.1</warmupTime>
        <targetParams>
          <canTargetBuildings>false</canTargetBuildings>
          <neverTargetIncapacitated>true</neverTargetIncapacitated>
        </targetParams>
        <soundCast>PsychicShockLanceCast</soundCast>
        <soundAiming>PsychicArtifactWarmupSustained</soundAiming>
      </li>
    </verbs>
    <comps>
      <li Class="CompProperties_ApparelReloadable">
        <maxCharges>500</maxCharges>
        <destroyOnEmpty>true</destroyOnEmpty>
        <hotKey>Misc4</hotKey>
        <chargeNoun>charge</chargeNoun>
        <displayGizmoWhileUndrafted>false</displayGizmoWhileUndrafted>
      </li>
      <li>
        <compClass>CompTargetEffect_PsychicShock</compClass>
      </li>
      <li Class="CompProperties_TargetEffect_FleckOnTarget">
        <fleckDef>PsycastPsychicEffect</fleckDef>
      </li>
      <li Class="CompProperties_TargetEffect_FleckConnecting">
        <fleckDef>PsycastPsychicLine</fleckDef>
      </li>
    </comps>
  </ThingDef>

<!-- Z_Enhancer -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>Z_Enhancer</defName>
    <label>Z_Enhancer</label>
    <labelNoun>a Z_Enhancer</labelNoun>
    <description>An installed Z_Enhancer.</description>
    <descriptionHyperlinks><ThingDef>Z_Enhancer</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>Z_Enhancer</spawnThingOnRemoved>
    <stages>
      <li>
        <naturalHealingFactor>1.5</naturalHealingFactor>
        <totalBleedFactor>0.5</totalBleedFactor>
        <statOffsets>
          <ResearchSpeed>0.25</ResearchSpeed>
          <GlobalLearningFactor>0.4</GlobalLearningFactor>
          <ImmunityGainSpeed>0.2</ImmunityGainSpeed>
          <InjuryHealingFactor>2</InjuryHealingFactor>
          <ArmorRating_Sharp>1.5</ArmorRating_Sharp>
          <ArmorRating_Blunt>1.5</ArmorRating_Blunt>
          <ArmorRating_Heat>1.5</ArmorRating_Heat>
          <RestFallRateFactor>-0.2</RestFallRateFactor>
          <MoveSpeed>0.25</MoveSpeed>
          <WorkSpeedGlobal>0.25</WorkSpeedGlobal>
          <ToxicEnvironmentResistance>0.3</ToxicEnvironmentResistance>
          <ToxicResistance>0.5</ToxicResistance>
          <IncomingDamageFactor>0.5</IncomingDamageFactor>
          <MeleeDamageFactor>1.5</MeleeDamageFactor>
          <MechBandwidth>12</MechBandwidth>
          <MechControlGroups>1</MechControlGroups>
          <WorkSpeedGlobalOffsetMech>0.1</WorkSpeedGlobalOffsetMech>
        </statOffsets>
        <blocksMentalBreaks>true</blocksMentalBreaks>
        <makeImmuneTo>
          <li>Flu</li>
          <li>Malaria</li>
          <li>SleepingSickness</li>
          <li>Plague</li>
          <li>WoundInfection</li>
          <li>LungRot</li>
          <li>GutWorms</li>
          <li>MuscleParasites</li>
          <li>OrganDecay</li>
        </makeImmuneTo>
        <hungerRateFactor>0.25</hungerRateFactor>
        <foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
      </li>
    </stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartBionicImperialBase">
    <defName>Z_Enhancer</defName>
    <label>Z_Enhancer</label>
    <description>A Z_Enhancer.</description>
    <descriptionHyperlinks><RecipeDef>InstallZ_Enhancer</RecipeDef></descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Health/HealthItem</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.80</drawSize>
      <color>(189,169,118)</color>
    </graphicData>
    <costList>
      <Z_StanStoneA>1</Z_StanStoneA>
    </costList>
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
        <li>ElectricSmithy</li>
        <li>FueledSmithy</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
    </recipeMaker>
    <generateCommonality>0</generateCommonality>
    <generateAllowChance>0.00</generateAllowChance>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantBase">
    <defName>InstallZ_Enhancer</defName>
    <label>install Z_Enhancer</label>
    <description>Install a Z_Enhancer.</description>
    <descriptionHyperlinks>
      <ThingDef>Z_Enhancer</ThingDef>
      <HediffDef>Z_Enhancer</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing Z_Enhancer.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Z_Enhancer</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Z_Enhancer</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>Z_Enhancer</addsHediff>
  </RecipeDef>
  
  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>RemoveZ_Enhancer</defName>
    <label>remove Z_Enhancer</label>
    <description>Remove Z_Enhancer.</description>
    <descriptionHyperlinks>
      <ThingDef>Z_Enhancer</ThingDef>
      <HediffDef>Z_Enhancer</HediffDef>
    </descriptionHyperlinks>
    <jobString>Removing  Z_Enhancer.</jobString>
    <removesHediff>Z_Enhancer</removesHediff>
  </RecipeDef>

</Defs>
